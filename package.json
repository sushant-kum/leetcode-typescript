{
  "name": "leetcode-typescript",
  "version": "1.0.0",
  "description": "Leetcode problems solved using TypeScript.",
  "scripts": {
    "prepare": "husky",
    "start": "tsx",
    "start:dev": "nodemon",
    "debug": "tsx --inspect-brk=5858",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier *.{css,html,js,json,md,scss,ts}",
    "format:check": "prettier --check ./**/*.{css,html,js,json,md,scss,ts}",
    "format:fix": "prettier --write ./**/*.{css,html,js,json,md,scss,ts}",
    "tool:commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushant-kum/leetcode-typescript.git"
  },
  "keywords": [
    "leetcode",
    "typescript",
    "leetcode-problems",
    "leetcode-solutions"
  ],
  "author": "Sushant Kumar <sushant.kum96@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sushant-kum/leetcode-typescript/issues"
  },
  "homepage": "https://github.com/sushant-kum/leetcode-typescript#readme",
  "dependencies": {
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.16.11",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.3.2",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "pretty-quick": "^4.0.0",
    "typescript-eslint": "^8.8.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
